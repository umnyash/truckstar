extends layout/layout

block variables
  - var title = 'Запчасти для Man TGX II 2017 г.'
  - var pageClass = 'catalog-page'

block content
  +breadcrumbs({ links: [
    {
      text: title,
    }
  ]})
  +catalog({
    filter: {
      modelSelection: false,
      compact: true
    },
  })
  +products({ heading: '<b class="heading__accent">Популярные</b> запчасти', products })
  +offer
