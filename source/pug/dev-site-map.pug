extends layout/layout

block variables
  - var title = 'Карта сайта'
  - var pageClass = 'site-map-page'

block content
  +breadcrumbs({ links: [
    {
      text: 'Карта сайта'
    }
  ]})
  .container
    ol
      li #[a(href='collections.html') Все разборки]
      li #[a(href='collections-empty.html') Все разборки (ничего не найдено)]
      li #[a(href='index.html') Главная]
      li #[a(href='index-not-authorized.html') Главная для не авторизованного (открывается модальное окно, вместо меню)]
