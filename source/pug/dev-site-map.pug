      li #[a(href='cart.html') Корзина]
      li #[a(href='cart-empty.html') Корзина (пустая)]
      li #[a(href='contacts.html') Контакты]
      li #[a(href='account.html') Личный кабинет]
      li #[a(href='account-empty.html') Личный кабинет (нет заказов)]
      li #[a(href='company.html') О нас]
      li #[a(href='reviews.html') Отзывы]
      li #[a(href='reviews-empty.html') Отзывы (ничего не найдено)]
      li #[a(href='search.html') Поиск]
